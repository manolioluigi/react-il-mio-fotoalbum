generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Photo {
  id          Int        @id @default(autoincrement())
  title       String
  description String?
  imageUrl    String?
  imageBytes  Bytes?
  visible     Boolean
  categories  Category[]
}

model Category {
  id     Int     @id @default(autoincrement())
  name   String
  photos Photo[]
}
